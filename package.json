{
  "name": "leancoffee-powerup",
  "version": "0.1.0",
  "description": "Everything you need to conduct a lean coffee session in Trello!",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx webpack-serve ./webpack.dev.js",
    "dist": "export NODE_ENV=production && webpack --config webpack.prod.js",
    "deploy": "npm run dist && npx s3-deploy './dist/**' --cwd './dist/' --gzip html,js,svg --deleteRemoved --etag --region=eu-west-1 --bucket $TRELLO_POWERUPS_BUCKET_NAME --profile $TRELLO_POWERUPS_PROFILE"
  },
  "keywords": [
    "trello",
    "power-up"
  ],
  "author": "Angelo Tata <angelo.tata@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "format-duration": "^1.1.0",
    "s3-deploy": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "fast-async": "^6.3.7",
    "html-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.1",
    "webpack-merge": "^4.1.3",
    "webpack-serve": "^1.0.4"
  }
}
